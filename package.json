{
    "name": "easy-format",
    "version": "1.2.0",
    "dependencies": {
        "ocamlfind": "andreypopp/ocamlfind#esy",
        "opam-installer-bin": "andreypopp/opam-installer-bin"
    },
    "esy": {
        "build": [
            "make",
            "make install",
            "(opam-installer --prefix=$cur__install || true)"
        ],
        "exportedEnv": {
            "easy_format_installed": {
                "scope": "global",
                "val": "true"
            },
            "easy_format_version": {
                "scope": "global",
                "val": "1.2.0"
            },
            "easy_format_enable": {
                "scope": "global",
                "val": "enable"
            }
        }
    },
    "peerDependencies": {
        "ocaml": "andreypopp/ocaml#esy"
    }
}
